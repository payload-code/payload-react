{
  "name": "payload-react",
  "version": "1.0.0",
  "description": "A simple react wrapper around Payload.js. See https://docs.payload.co for more information.",
  "main": "dist/payload-react.js",
  "module": "dist/payload-react.js",
  "scripts": {
    "test": "jest",
    "start": "webpack --watch",
    "clean": "rm -rf dist",
    "build": "npm run clean && NODE_ENV=production babel src/ --out-dir dist --copy-files",
    "prepare": "husky install",
    "eslint": "eslint src --ext .js --fix",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "keywords": [],
  "author": "Payload",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/payload-code/payload-react.git"
  },
  "homepage": "https://github.com/payload-code/payload-react#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.3",
    "babel-loader": "^8.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "minimist": ">=1.2.2",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.6.12",
    "prettier": "^3.0.0",
    "regenerator-runtime": "^0.13.11",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./tests/setup-tests.js"
    ],
    "globals": {
      "window": {}
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --quiet --cache --fix"
    ],
    "*.{js}": [
      "prettier --write"
    ]
  },
  "files": [
    "dist"
  ]
}
